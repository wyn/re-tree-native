{
  "name": "re-tree",
  "version": "0.1.0",
  "description": "Tree structure with arbitrary parent/children",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "pesy",
    "release": { "releasedBinaries": [ "ReTreeApp.exe" ] }
  },
  "buildDirs": {
    "test": {
      "require": [ "re-tree.lib", "qcheck" ],
      "main": "TestReTree",
      "name": "TestReTree.exe"
    },
    "library": {
      "name": "re-tree.lib",
      "namespace": "ReTree"
    },
    "executable": {
      "require": [ "re-tree.lib" ],
      "main": "ReTreeApp",
      "name": "ReTreeApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestReTree.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/dune": ">=1.6.0",
    "@opam/qcheck": "0.13",
    "ocaml": "^4.4.0",
    "qcheck": "^0.0.3"
  },
  "devDependencies": {
    "ocaml": "4.7.1004",
    "refmterr": "*",
    "pesy": "*",
    "@opam/merlin": "*",
    "qcheck": "^0.0.3"
  }
}
